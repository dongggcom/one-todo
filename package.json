{
  "name": "one-todo",
  "version": "0.1.0",
  "description": "a vscode plugin about todo list",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongggcom/one-todo.git"
  },
  "keywords": [
    "vscode",
    "plugin",
    "todo"
  ],
  "icon": "images/one-todo@128x128.png",
  "author": "dongggcom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dongggcom/one-todo/issues"
  },
  "homepage": "https://github.com/dongggcom/one-todo#readme",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "one-todo插件设置",
      "properties": {
        "oneTodo.showHomepage": {
          "type": "boolean",
          "default": true,
          "description": "是否启动时显示首页"
        }
      }
    },
    "commands": [
      {
        "command": "catCoding.start",
        "title": "Start new cat coding session",
        "category": "Cat Coding"
      },
      {
        "command": "extension.sayHello",
        "title": "hello!"
      },
      {
        "command": "extension.main.showHomepage",
        "title": "显示首页"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "one-todo-icon",
          "title": "TODO",
          "icon": "images/one-todo.svg"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.sayHello",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "one-todo-icon": [
        {
          "id": "todo",
          "name": "TODO列表"
        },
        {
          "id": "un-todo",
          "name": "UN-TODO列表"
        },
        {
          "id": "other",
          "name": "其他"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.34.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
